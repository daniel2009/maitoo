.c-alert-md {width:550px;background-color:#fff;text-align:left}
.c-alert-md .hd {border-bottom:1px solid #efefef}
.c-alert-md .bd {line-height:2}
.c-alert-md .hd, .c-alert-md .bd,.c-alert-md .bt {padding:10px;}
