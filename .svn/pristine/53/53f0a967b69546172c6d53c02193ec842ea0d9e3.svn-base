*{margin:0;padding:0}
body {height:100%}
body { font: 12px/1.6 "Microsoft YaHei",arial,sans-serif; color: #666; word-wrap: break-word; word-break: break-all; background: #eee; text-align: center; }
table {border-collapse:collapse;border-spacing:0;}
ol, ul {list-style:none outside}
h1, h2, h3, h4, h5, h6 {font-size:100%;}
fieldset, img {border:0 none;}
img{vertical-align: middle;}
q:before, q:after {   content: '';}  
button{padding:0;cursor:pointer;vertical-align:middle;}
select,input,textarea{font:12px Tahoma, Arial, simsun;line-height:1.2em;}
i,em,cite,b{ font-style:normal}
a {text-decoration:none}

@font-face {font-family: 'iconfont';
    src: url('/content/iconfont/iconfont.eot'); /* IE9*/
    src: url('/content/iconfont/iconfont.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
    url('/content/iconfont/iconfont.woff') format('woff'), /* chrome、firefox */
    url('/content/iconfont/iconfont.ttf') format('truetype'), /* chrome、firefox、opera、Safari, Android, iOS 4.2+*/
    url('/content/iconfont/iconfont.svg#uxiconfont') format('svg'); /* iOS 4.1- */
}

.xfont{
    font-family:"iconfont" !important;
    font-size:16px;font-style:normal;
    -webkit-font-smoothing: antialiased;
    -webkit-text-stroke-width: 0.2px;
    -moz-osx-font-smoothing: grayscale;
}
.empty-container,.empty-top, .empty-content, .empty-bottom {width:100%;}
/*.empty-top, .empty-content, .empty-bottom {text-align:left;}*/
.empty-area {text-align:left;width:1200px;margin:0 auto;}
.empty-top {height:70px;background-color:#fff;border-bottom:1px solid #CCC;}
.versions {color:#999;}
.versions a {font:normal 12px/1.6 "Microsoft YaHei";color:#999;}
.versions .hover {display:inline-block;background-color:#999;color:#fff;padding:1px 8px;margin-right:-8px}
.empty-content {background:url(/imag/empty/content-bg.jpg) 0 0 repeat-x;padding:30px 0 0 0}
.empty-bottom {height:80px;background-color:#494039;border-top:1px solid #B77719;text-align:center;padding-top:20px}
.empty-bottom .introduce a {display:inline-block;margin:0 40px}


.c-forms .item {margin:15px 0}
.c-forms .c-forms-label {margin-bottom:5px}
.c-forms-label{font:bold 14px/1.6 "微软雅黑";}
.c-input-text,.c-md-select {display:inline-block;vertical-align:middle;width:250px;height:26px;line-height:26px;padding:0 5px;border:1px solid #DADADA;box-shadow: 2px 2px 4px #F0F0F0 inset; border-top:1px solid #bababa;}
.c-md-select{width:263px;height:30px;line-height:30px;padding:0;vertical-align:middle}
.c-btn-lg-next,.c-btn-md-yel {display:inline-block;height:40px;line-height:40px;border:0;text-align:center;width:200px;font-size:14px;color:#fff;font-weight:bold;background-color:#B7771A;cursor:pointer}
.c-btn-lg-next-after,.c-btn-md-yel-shadow {content:'.';display:block;width:200px;height:19px;background:url(/imag/empty/c-btn-lg-next-bg.png) no-repeat;margin-bottom:10px;font-size:0}
.c-dashed {height:0;font-size:0;border-bottom:1px dashed #ccc;clear:both;}
.c-btn-gray {display:inline-block;border-radius:4px;padding:0 12px;background: -moz-linear-gradient(top, #fff,#efefef,#e2e2e2);background:-webkit-gradient(linear, 0% 0%, 0% 100%, from(#fff), to(#e2e2e2));background: -o-linear-gradient(top,#fff, #e2e2e2);filter:progid:DXImageTransform.Microsoft.Gradient(gradientType='0',startColorStr='#ffffff',endColorStr='#e2e2e2');background:linear-gradient(to bottom, #fff,#efefef,#e2e2e2);color:#666;border:1px solid #ccc;}
.c-btn-md-yel {height:40px;line-height:40px;}
.c-loginprocess {height:44px;}
.c-loginprocess li {float:left;width:16.6666%;height:44px;background:url(/imag/empty/gradient.jpg) right bottom no-repeat;}
.c-loginprocess .last {background:none}
.c-loginprocess span {display:block;padding:0 10px;border-bottom:6px solid #F0E7D9;font:normal 14px/1.6 "Microsoft yahei";color:#999;margin-top:18px}
.c-loginprocess .passed span {border-color:#D2B284;color:#D2B284}

.c-hr-title{height:30px;border-bottom:1px solid #ddd;}
.c-hr-title b {font:bold 14px/30px "Microsoft yahei";color:#000}
/*密码强弱*/
.c-password-range{width:265px;height:14px;color:#fff;text-align:left;font:normal 10px/1 'Microsoft yahei';font-size:0;letter-spacing:-4px}
.c-password-range .item{display:inline-block;font-size:10px;letter-spacing:normal;vertical-align:top;text-align:center;margin:0;border-right:1px solid #fff;width:32.6%;height:14px;white-space:nowrap;background-color:#D2B284;} 

