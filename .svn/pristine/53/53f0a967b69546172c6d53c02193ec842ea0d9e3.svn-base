.cont-icon {margin:0 20px;vertical-align:top;font-size:46px;line-height:1.2}

.helpIcon{display:inline-block;cursor:pointer;width:14px;height:15px;margin-bottom:-3px;background:url(/img/icon.png) no-repeat -142px -269px}
.tipIcon{display:none;background:url(/img/TipNewLong.png) no-repeat 0 0;width:408px;height:80px;position:absolute}
.tipIcon .con{width:310px;height:80px;margin:15px 0 0 14px}
.tipIcon .tit{color:#EB5E00}
.tipShut{width:10px;height:10px;display:block;position:absolute;right:28px;top:12px;}


/*jquery lib reset*/
/*.ui-dialog .ui-dialog-content {padding:0}*/
.ui-widget-header {background:#fff;border:0;border-bottom:1px solid #efefef}
.ui-dialog {padding:0}
/* states and images */
.ui-icon { width: 16px; height: 16px; background-image: url(/Imag/ui-icons_454545_256x240.png); }
.ui-widget-content .ui-icon {background-image: url(/Imag/ui-icons_454545_256x240.png); }
.ui-widget-header .ui-icon {background-image: url(/Imag/ui-icons_454545_256x240.png); }
.ui-state-default .ui-icon { background-image: url(/ui-icons_454545_256x240.png); }
.ui-state-hover .ui-icon, .ui-state-focus .ui-icon {background-image: url(/Imag/ui-icons_454545_256x240.png); }
.ui-state-active .ui-icon {background-image: url(/Imag/ui-icons_454545_256x240.png); }
.ui-state-highlight .ui-icon {background-image: url(/Imag/ui-icons_454545_256x240.png); }
.ui-state-error .ui-icon, .ui-state-error-text .ui-icon {background-image: url(/Imag/ui-icons_454545_256x240.png); }
.ui-state-hover, .ui-widget-content .ui-state-hover, .ui-widget-header .ui-state-hover, .ui-state-focus, .ui-widget-content .ui-state-focus, .ui-widget-header .ui-state-focus { border: 1px solid #444; background: #efefef; font-weight: bold; color: #ffffff; }