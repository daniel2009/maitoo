<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%><%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%><!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"><html xmlns="http://www.w3.org/1999/xhtml"><head><link rel="shortcut icon" href="img/favicon.ico" /></head><body>		<div class="block">		<p class="block-heading" data-toggle="collapse"			data-target="#chart-container" style="font-size:20px">列表-收件地址</p>		<div class="box_content">			<div class="content_title">列表-收货地址</div>			<div class="content_text">				<div class="tab_box">					<div class="query">						<a class="btn" href="javascript:nav741Click()" target="_blank"							style="background: #007FFF;color: #FFFFFF;padding: 5px 15px;display:inline">增加</a>					</div>					<table id="sjdztable" class="table">						<thead>							<tr>								<th style="width:30px">序号</th>								<th style="width:40px">收货人</th>								<th style="width:40px">电话</th>								<th style="width:110px">国家/地区</th>								<th>详细地址</th>								<th style=" width:60px">身份证号</th>								<th style="width:70px">身份证正面图</th>								<th style="width:70px">身份证反面图</th>								<th style="width:70px">身份证合成图</th>								<th style="width:60px;">操作</th>							</tr>						</thead>						<tfoot>							<tr>								<td colspan="8" align="right">共<span id="trsize">0</span>条									<a id="first_page" href="1" name="to_page">首页</a> | <a									id="pre_page" name="to_page">上一页</a> | <input type="text"									class="bor" size="2" /> /<span id="tpsize">0</span>| <a									id="next_page" name="to_page">下一页</a> | <a id="last_page"									name="to_page">未页</a>								</td>							</tr>						</tfoot>						<tbody>						</tbody>					</table>				</div>			</div>		</div>	</div>	<div style="display: none;" id="tmpmsgdiv"></div>	<div class="block" id="add">		<p class="block-heading" data-toggle="collapse"			data-target="#chart-container" style="font-size:20px">增加-收件地址</p>		<div class="box_content">			<div class="content_title">增加-收货地址 </div>			<div class="content_text">				<form action="" id="add_form" method="post"					enctype="multipart/form-data">					<table class="itable">						<tr>							<td class="align_right">收货人：</td>							<td class="align_left"><input name="name" type="text"								maxlength="10" /><span class="red">*</span></td>						</tr>						<tr>							<td class="align_right">收货地址：</td>							<td class="align_left">								<input type="radio" name="country" value="1" checked="checked" />中国 								<input type="hidden" name="province" /> 								<input type="hidden" name="city" /> 								<input type="hidden" name="dist" /> 								<select id="loc_province" style="width:150px"></select> / 								<select id="loc_city" style="width:150px"></select> / 								<select id="loc_town" style="width:150px"></select>																&nbsp;&nbsp;禁用											<input type="checkbox" name="use_pct" id="use_pct" value="">								<!--  <br />								<input type="radio" name="country" value="2" />international -->								<!-- <input type="hidden" name="province" /> 								<input type="hidden" name="city" />								<input type="hidden" name="dist" />  -->								<!-- <select id="usa_province" style="width:150px"></select> / 								<select id="usa_city" style="width:150px"></select>--> <!-- /<select id="usa_town"style="width:150px"></select> -->							</td>						</tr>						<tr>							<td class="align_right">详细地址：</td>							<td class="align_left"><input type="text" name="sadd"								maxlength="36" /><span class="red">*</span>							</td>						</tr>						<tr>							<td class="align_right">手机：</td>							<td class="align_left"><input name="phone" type="text"								maxlength="12" /><span class="red">*</span></td>						</tr>						<tr>							<td class="align_right">邮编：</td>							<td class="align_left"><input name="zcode" type="text"								maxlength="10" />							</td>						</tr>						<tr>							<td class="align_right">身份证号：</td>							<td class="align_left"><input name="cid" type="text"								maxlength="18" /></td>						</tr>						<tr>							<td class="align_right">上传身份证正面图片：</td>							<td class="align_left"><input name="file" type="file" />								（上传支持JPG，PNG，BMP等图片格式，最大不超过4MB）</td>													</tr>						<tr>							<td class="align_right">上传身份证正反图片：</td>							<td class="align_left"><input name="fileother" type="file" />								（上传支持JPG，PNG，BMP等图片格式，最大不超过4MB）</td>													</tr>						<!--  <tr>							<td class="align_right"></td>-->														<!--  <td class="align_left"><a class="btn" href="javascript:zengjiadizi()">保存</a></td>  -->						<!-- </tr>-->					</table>						<div style="text-align:center;margin-top:10px;">						<input id="add_zengjiadizi_bt" class="button" type="button"									value="保存" onClick="zengjiadizi()" style="background: #007FFF;color: #FFFFFF;padding: 5px 15px;display:inline"/> 						</div>									</form>			</div>		</div>	</div><script type="text/javascript" src="${pageContext.request.contextPath }/resources/scripts/jquery.form.js"></script> </body></html>