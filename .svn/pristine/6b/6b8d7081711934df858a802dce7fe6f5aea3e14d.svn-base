/*nagation*/
.navigation{
	height: 52px;
	background-color: #ececeb;
}
.navigation .nav-menu li {
	float: left;
	margin-right: 3px;
}
.navigation .nav-menu a {
	display: block;
	padding: 0 22px 5px;
