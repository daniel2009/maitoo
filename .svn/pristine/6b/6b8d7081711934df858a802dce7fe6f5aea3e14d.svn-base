/*广告*/
.view {
	width: 900px;
	margin: 0 auto;
}
/*新闻*/
.news {
	width: 900px;
	margin: 10px auto;
}
.q_select {
	border: 1px solid #ccc;
	width: 250px;
	height: 260px;
	float: left;
	
}
.q_select .q_select_top {
	background: url(../img/top_bg.png);
	height: 30px;
	border-bottom: 1px #ccc solid;
}
.q_select .q_select_top .top_title {
	width: 50%;
	border-right: 1px #ccc solid;
	height: 31px;
	line-height: 30px;
	text-align: center;
	
}
.q_select .q_select_content {
	height: 230px;
}
.q_select_content .inputs {
	height: 160px;
	padding: 10px;
}
.q_select_content .select_btn {
	height: 50px;
	text-align: center;
	
}
.q_select_content .select_btn a {
	height: 30px;
	width: 71px;
	text-align: center;
	line-height: 30px;
	background: #00BFFF; /*url(../img/index/btn.png);*/
	background-size: 71px 30px;
	display: inline-block;
	margin: 9px 0;
	color: #FFFFFF;
}
.new {
	height: 244px;
	width: 610px;
	border: 1px solid #ccc;
	padding: 8px;
	padding-right: 0px;
	margin-left: 0px;
	float: right;
}
.mt_notice {
	width: 300px;
	;
	float: left;
	height: 240px;
}
.mt_news {
	width: 300px;
	float: left;
	border-left: 1px #ccc solid;
	height: 240px;
	padding-left: 8px;
}
.mt_notice .title, .mt_news .title {
	height: 30px;
	line-height: 30px;
	border-bottom: 1px solid #ccc;
	margin-right: 8px;
	background: #E0EEEE;
}
.mt_notice .title img, .mt_news .title img {
	margin: 9px;
	vertical-align: middle;
}
.mt_notice .news_list ul li, .mt_news .news_list ul li {
	height: 34px;
	line-height: 34px;
	border-bottom: 1px solid #ccc;
	list-style: square inside url(../img/clist_style.png);
	margin-right: 8px;
}
.mt_notice .foot, .mt_news .foot {
	line-height: 34px;
	margin-right: 8px;
	background: #E0EEEE;
}
.mt_notice .foot a, .mt_news .foot a {
	float: right;
}
/*流程*/
