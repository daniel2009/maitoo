<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%><%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%><!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"><html xmlns="http://www.w3.org/1999/xhtml"><head><link rel="shortcut icon" href="img/favicon.ico" /></head><body>		<div style="display: none;" id="tmpmsgdiv"></div>	<div class="block" id="add">		<p class="block-heading" data-toggle="collapse"			data-target="#chart-container" style="font-size:20px">自助打单</p>		<div class="box_content">			<div class="content_title">自助打单 </div>			<div class="content_text">				<form action="" id="add_form_endicial_label" method="post"					enctype="multipart/form-data">					<table class="itable">						<tr>							<td class="align_right">发件人信息：</td>							<td class="align_left"><label>姓名：</label><input name="s_name" type="text"								/><label>&nbsp;&nbsp;电话：</label><input name="s_phone" type="text"								maxlength="18" /></td>						</tr>						<tr>							<td class="align_right">发件人地址：</td>							<td class="align_left"><label>地址：</label><input name="s_addr" type="text"								style="width:50%" /><br/><label>城市：</label><input name="s_city" type="text"								style="max-width:15%" /><label>&nbsp;&nbsp;州：</label><input name="s_state" type="text"								style="max-width:15%" /><label>&nbsp;&nbsp;邮编：</label><input name="s_zipcode" type="text"								style="max-width:8%" maxlength="5"/>&nbsp;&nbsp;附加邮编：</label><input name="s_zip4" type="text"								style="max-width:5%" maxlength="4" /></td>						</tr>						<tr>							<td class="align_right">收件人信息：</td>							<td class="align_left"><label>姓名：</label><input name="r_name" type="text"								maxlength="10" /><label>&nbsp;&nbsp;电话：</label><input name="r_phone" type="text"								maxlength="18" /><label>&nbsp;&nbsp;公司名称：</label><input name="r_toCompany" type="text"								/></td>						</tr>						<tr>							<td class="align_right">收件人地址：</td>							<td class="align_left"><label>国家：</label><select name="tocountry">							<option value="US" name="United States">美国</option>							<option value="CN" name="China">中国</option>							</select>							    <label><br/>地址：</label><input name="r_addr" type="text"								style="width:50%" /><br/><label>城市：</label><input name="r_city" type="text"								style="max-width:15%" /><label>&nbsp;&nbsp;州：</label><input name="r_state" type="text"								style="max-width:15%" /><label>&nbsp;&nbsp;邮编：</label><input name="r_zipcode" type="text"								style="max-width:8%" maxlength="5"/>&nbsp;&nbsp;附加邮编：</label><input name="r_zip4" type="text"								style="max-width:5%" maxlength="4" /></td>						</tr>												<tr>							<td class="align_right">包裹信息：</td>							<td class="align_left"><label>物品描述</label>								<textarea name="description" type="text"								style="width:80%"/>								<br/><label>重量（磅，lb）：</label><input name="weight" type="text"								style="max-width:15%" /><!-- <label>&nbsp;&nbsp;重量(oz)：</label><input name="weightoz" type="text"								style="max-width:15%" /> --><label>&nbsp;&nbsp;物品数量：</label><input name="quantity" type="text"								style="max-width:15%" />							</td>						</tr>						<tr>							<td class="align_right">包裹价值($)：</td>							<td class="align_left">								<input name="value" type="text"								style="max-width:15%" /><span>美元&nbsp;&nbsp;</span>																<label>邮件类型：</label><select name="MailClass">							<option value="StandardPost">标准邮件</option>							<option value="PriorityExpress">特快专递</option>							<option value="First">第一类邮件包裹</option>							<option value="LibraryMail">图书馆邮件</option>							<option value="MediaMail">媒体邮件</option>							<option value="ParcelSelect">轻型包裹</option>							<option value="Priority">优先邮件</option>							<option value="CriticalMail">重要邮件</option>											</select>							</td>						</tr>											</table>						<div>						<input id="add_zengjiadizi_bt" class="button" type="button"									value="提交" onClick="Endicia_label_submit()" style="background: #007FFF;color: #FFFFFF;padding: 5px 15px;display:inline"/> 						</div>									</form>			</div>							<!-- 0902 add by kai,添加 网上置单数据-->		<div class="box_content">			<div class="content_title">打单记录</div>			<div class="content_text">				<form action="" id="online_order_history" method="post"					enctype="multipart/form-data">					<table class="itable">						<tr>							<td class="align_right">打单历史：</td>							<td class="align_left">								<table id="endicialabel_history" class="mtable">									<thead>										<tr>											<td align="center">订单号</td>											<td>创建时间</td>											<td>发件人</td>											<td>发件人电话</td>											<td colspan="4">发件人地址</td>											<td >收件人</td>											<td>收件人电话</td>											<td colspan="4">收件人地址</td>											<td>费用</td>											<td>操作</td>																					</tr>									</thead>									<tbody>									</tbody>									<tfoot>										<tr>											<td colspan="16" align="center">共<span id="trsize1">0</span>条												<a id="first_page1" href="1" name="to_page">首页</a> | <a												id="pre_page1" name="to_page">上一页</a> | <input type="text"												class="bor" size="2" style="width:20px;height:10px;" /> /<span												id="tpsize1">0</span>| <a id="next_page1" name="to_page">下一页</a>												| <a id="last_page1" name="to_page">未页</a>											</td>										</tr>									</tfoot>								</table>							</td>						</tr>					</table>				</form>			</div>		</div>											</div>	</div></body></html>