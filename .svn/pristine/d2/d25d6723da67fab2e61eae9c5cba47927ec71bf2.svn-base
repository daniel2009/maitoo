<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%><!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"><html>	<head>	<link rel="shortcut icon" href="../img/favicon.ico" /> 	</head>	<body>	<div class="block">		<p class="block-heading" data-toggle="collapse"			data-target="#chart-container" style="font-size:20px">添加仓库			<a href="#" id="a_back_endiciallabel_list_link" class="button">返回列表</a>			</p>		<div class="box_content">			<div class="content_title">添加仓库</div>			<div class="content_text">						<div class="tab-content" id="tab2">		          		          <form action="" id="add_form_warehose" method="post" enctype="multipart/form-data">		            <fieldset>		            <input type="hidden" name="widtype1" value="0"/>			            <p>			              <label class="cllabel">发件人-姓名：</label>			              <input class="text-input small-input" type="text" name="s_name" /><span class="gl_message">*</span>			             </p>			              <p>			              <label class="cllabel">发件人-电话：</label>			              <input class="text-input small-input" type="text" name="s_phone" /><span class="gl_message">*</span>			            </p>			            <p>			              <label class="cllabel">发件人-地址：</label>			              <input class="text-input small-input" type="text" name="s_addr" />			              </p>			              <p>			              <label class="cllabel">发件人-城市：</label>			              <input class="text-input small-input" type="text" name="s_city" />			              </p>			              <p>			              <label class="cllabel">发件人-州：</label>			              <input class="text-input small-input" type="text" name="s_state" />			              </p>			              <p>			              <label class="cllabel">邮编/附加邮编：</label>			              <input class="text-input small-input" type="text" name="s_zipcode" maxlength="5" />			              <input class="text-input small-input" type="text" style="max-width:50px" name="s_zip4" maxlength="4"/>			            </p>			              <p>			              <label class="cllabel">收件人-姓名：</label>			              <input class="text-input small-input" type="text" name="r_name" /><span class="gl_message">*</span>			              </p>			              <p>			              <label class="cllabel">收件人-电话：</label>			              <input class="text-input small-input" type="text" name="r_phone" /><span class="gl_message">*</span>			              </p>			              <p>			              <label class="cllabel">收件人-公司名：</label>			              <input class="text-input small-input" type="text" name="r_toCompany" /><span class="gl_message">*</span>			            </p>			            <p>			            	<span style="padding-left: 25px;">收件人-国家：</span><select name="tocountry">							<option value="US" name="United States">美国</option>							<option value="CN" name="China">中国</option>							</select>			             </p>			              <p> 			              <label class="cllabel">收件人-地址：</label>			              <input class="text-input small-input" type="text" name="r_addr" />			              </p>			              <p>			              <label class="cllabel">收件人-城市：</label>			              <input class="text-input small-input" type="text" name="r_city" />			              </p>			              <p>			              <label class="cllabel">收件人-州：</label>			              <input class="text-input small-input" type="text" name="r_state" />			              </p>			              <p>			              <label class="cllabel">邮编/附加邮编：</label>			              <input class="text-input small-input" type="text" name="r_zipcode" maxlength="5"/>			              <input class="text-input small-input" type="text" name="r_zip4" maxlength="4" style="max-width:50px"/>			            </p>			            <p>			               <label class="cllabel">物品描述:</label>								<textarea name="description" type="text"								style="width:50%"/>								<br/>																<label class="cllabel">重量:</label><input name="weight" type="text"								style="max-width:15%" /><span>（磅，lb）</span>								 </p>			              <p>								<label class="cllabel">物品数量：</label><input name="quantity" type="text"								style="max-width:15%" />			            </p>			            <p>			              			             <label class="cllabel">包裹价值($)：</label>			              <input class="text-input small-input" type="text" name="value" />			              </p>			              <p>			              <label class="cllabel">邮件类型：</label>			              <select name="MailClass">							<option value="StandardPost">标准邮件</option>							<option value="PriorityExpress">特快专递</option>							<option value="First">第一类邮件包裹</option>							<option value="LibraryMail">图书馆邮件</option>							<option value="MediaMail">媒体邮件</option>							<option value="ParcelSelect">轻型包裹</option>							<option value="Priority">优先邮件</option>							<option value="CriticalMail">重要邮件</option>											</select>			            </p>			            			            <p style="margin-left: 50px;">			              <input id="bt_add_ediciallabel" class="button" type="submit" value="提交" />			              <input class="button" type="reset" value="重置">			            </p>		            </fieldset>		            <div class="clear"></div>		            <!-- End .clear -->		          </form>		        </div>			</div>		</div>		</div>				<div id="tmpmsgdiv1" style="display:none;"></div>	</body><script type="text/javascript">	$("#main-content").ready(function() {		Endicia_label_page_init_admin();	});</script></html>