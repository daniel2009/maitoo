<script>
setFooter(".footer");
serachProductType();
searchProduct(0);
getCartCount();
function serachProductType(){
	$.ajax({
		type: "post",
		url: "../productType/findAll",
		success: function(response) {
			if(200==response.code){
				$("#productTypeUl").html("");
				string = "<li id='productTypeLi0'><a href='javascript: searchProduct(0);'>全部</a></li>";
				for(var i=0; i<response.data.datas.length; i++){
					string += "<li id='productTypeLi"+ response.data.datas[i].id + "'><a href='javascript: searchProduct(" + response.data.datas[i].id + ");'>" + response.data.datas[i].name + "</a></li>";
				}
				$("#productTypeUl").html(string);
			}
		}
	});
}
function searchProduct(typeId){
	var orignalTypeId = $("#activeProductType").val();
	$("#productTypeLi" + orignalTypeId).removeClass("active");
	var formData = new FormData();
	if(0 < typeId){
		formData.append("typeId", typeId);
	}
	$("#productTypeLi" + typeId).addClass("active");
	$("#activeProductType").val(typeId);
	loadProductList(formData);
}
function loadProductList(formData){
	$.ajax({
		type: "post",
		url: "../product/findAllByUser",
		data : formData,
		processData : false,
		contentType : false,
		success: function(response) {
			if(200==response.code ){
				showProductList(response.data.datas);
				showProductPage(response.data);
				clearWrongImgNoBaseUrl();
			}else if(36011 == response.code){
				showProductList(0);
				$("#pagination").html("");
			}
		}
	});
}
function showProductList(list){
	$("#productUl").html("");
	string = "";
	for(var i=0; i<list.length; i++){
		var product = list[i];
		string += "<li style='height: 500px;'>";
		string += "<img src='../" + product.pic1 + "' style='width: 250px; height: 250px;' alt=''/>";
		string += "<h5 style='max-height: 200px; overflow: hidden; '>" + product.name + "</h5>";
		var price = product.price;
		if(price == "" || price == -999999){
			price = "<span style='color: #f00;'>待定</span>";
		}
		string += " <p>" + product.currency + price + "</p>";
		string += "<div class='info_func'>";
		string += "<p>";
		string += "<a href='javascript:;'>0</a>";
		string += "<a href='javascript:;'>0</a>";
		string += "<a href='javascript:;'>" + product.commentNum + "</a>";
		string += "</p>";
		string += "<a href='javascript: getProductInfo(" + product.id + ");'>购物直达></a>";
		string += "</div>";
		string += "</li>";
	}
	$("#productUl").html(string);
}
function showProductPage(pageSplit){
	$("#pagination").html("");
	var string = "";
	if(pageSplit.pageNow-1 > 0){
		var pageNow = pageSplit.pageNow-1;
		string += "<a onclick='javascript: changePage(" + pageNow + ");'>上一页</a>";
	}
	var active = " class='active' ";
	for(var i=-2; i<3; i++){
		var pageNow = pageSplit.pageNow+i;
		if(pageNow > 0 && (pageNow < pageSplit.pageCount || pageNow == pageSplit.pageCount)){
			string += "<a " + (i==0 ? active : "") + (i==0 ? "" : " onclick='javascript: changePage(" + pageNow + ");' ") + ">" + pageNow + "</a>";
		}
	}
	if(pageSplit.pageNow+1 == pageSplit.pageCount || pageSplit.pageNow+1 < pageSplit.pageCount){
		var pageNow = pageSplit.pageNow+1;
		string += "<a onclick='javascript: changePage(" + pageNow + ");'>下一页</a>";
	}
	$("#pagination").html(string);
}
function changePage(pageIndex){
	var formData = new FormData();
	var typeId = $("#activeProductType").val();
	formData.append("pageIndex", pageIndex);
	if(0 < typeId){
		formData.append("typeId", typeId);
	}
	loadProductList(formData);
	return false;
}
function getCartCount(){
	$.ajax({
		type: "get",
		url: "../cart/countCart",
		success: function(response) {
			if(200==response.code || 36019==response.code){
				$("#cartCount").html(response.data);
			}else if(607 == response.code){
				var cartArray = localStorage.getItem("cartArray");
				if(cartArray != null){
					cartArray = JSON.parse(cartArray);
					$("#cartCount").html(cartArray.length);
				}
			}
		}
	});
}
function getProductInfo(productId){
	window.sessionStorage.setItem("productIdKeep", productId);
	location.href = "../91gou/product_detail.html";
}
function searchSpiderOrder(){
	sessionStorage.setItem("spiderOrderUrlKeep", $("#spiderOrderUrl").val());
	sessionStorage.removeItem("productIdKeep");
	location.href="../91gou/product_detail.html";
}
</script>
