<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%><%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%><!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"><html xmlns="http://www.w3.org/1999/xhtml"><head><link rel="shortcut icon" href="img/favicon.ico" /></head><body><div style="display: none;" id="tmpmsgdiv"></div>	<input type="hidden" name="id" value="${param.id }" />	<div class="block" id="update">		<p class="block-heading" data-toggle="collapse"			data-target="#chart-container" style="font-size:20px">修改-收件地址</p>		<div class="box_content">			<div class="content_title">修改-收货地址</div>			<div class="content_text">				<form action="" id="update_form" method="post"					enctype="multipart/form-data">					<input type="hidden" name="id" value="${param.id }" />					<table class="itable">						<tr>							<td class="align_right">收货人：</td>							<td class="align_left"><input name="name" type="text"								maxlength="10" /><span class="red">*</span></td>						</tr>						<tr>							<td class="align_right">收货地址：</td>							<td class="align_left"><input type="hidden" name="province" />								<input type="hidden" name="city" /> <input type="hidden"								name="dist" /> <select id="loc_province" style="width:150px">							</select> / <select id="loc_city" style="width:150px">							</select> / <select id="loc_town" style="width:150px">							</select>&nbsp;&nbsp;禁用<input type="checkbox" name="use_pct" id="use_pct" value="">														</td>						</tr>						<tr>							<td class="align_right">详细地址：</td>							<td class="align_left"><input type="text" name="sadd"								maxlength="36" /><span class="red">*</span>							</td>						</tr>						<tr>							<td class="align_right">手机：</td>							<td class="align_left"><input name="phone" type="text"								maxlength="12" /><span class="red">*</span></td>						</tr>						<tr>							<td class="align_right">邮编：</td>							<td class="align_left"><input name="zcode" type="text"								maxlength="10" />							</td>						</tr>						<tr>							<td class="align_right">身份证号：</td>							<td class="align_left"><input name="cid" type="text"								maxlength="18" />						</tr>						<tr>							<td class="align_right">上传身份证正面图片：</td>							<td class="align_left"><input id="cardpicid" name="file" type="file" /> （上传支持JPG，PNG，BMP等图片格式，最大不超过4MB <span								id="card_file"></span> <input name="old_file" type="hidden" />							</td>						</tr>						<tr>							<td class="align_right">上传身份证反面图片：</td>							<td class="align_left"><input id="cardpicidother" name="fileother" type="file" /> （上传支持JPG，PNG，BMP等图片格式，最大不超过4MB <span								id="card_fileother"></span> <input name="old_fileother" type="hidden" />							</td>						</tr>						<!--  <tr>							<td class="align_right"></td>-->							<!-- <td class="align_left"><a  class="btn" href="javascript:xiugaidizi()">保存</a></td>  -->						<!-- </tr> -->					</table>					<div>					<input id="add_zengjiadizi_bt" class="button" type="button"									value="保存" onClick="xiugaidizi()"/> 					</div>										</form>			</div>		</div>	</div></body></html>