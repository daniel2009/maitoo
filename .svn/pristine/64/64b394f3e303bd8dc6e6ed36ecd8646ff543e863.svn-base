<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%><html>	<head>	<link rel="shortcut icon" href="../img/favicon.ico" /> 	</head>	<body>	<div class="block">		<p class="block-heading" data-toggle="collapse"			data-target="#chart-container" style="font-size:20px">修改会员信息			<a href="#" id="a_back_user_list_link">返回列表</a>			&nbsp;&nbsp;|&nbsp;&nbsp;			<a href="#" id="a_update_user_pwd_link">修改会员密码</a>			</p>		<div class="box_content">			<div class="content_title">修改会员信息</div>			<div class="content_text">						<div class="tab-content" id="tab2">		          <form id="modify_user_form" action="#" method="post">		            <input type="hidden" name="id" value="${param.id }"/>		            <fieldset>		            	<p>			              <label style="display: inline;">用户编号：</label>			              <label style="display: inline;" id="u_m_id">${param.id }</label>			              <br/>			              <label style="display: inline;">注册类型：</label>			              <label style="display: inline;" id="u_reg_type">&nbsp;</label>			              <br/>			              <label style="display: inline;">电话状态：</label>			              <label style="display: inline;" id="u_phone_state">&nbsp;</label>			              <br/>			              <label style="display: inline;">邮箱状态：</label>			              <label style="display: inline;" id="u_email_state">&nbsp;</label>			              <br/>			              <label style="display: inline;">创建时间：</label>			              <label style="display: inline;" id="u_create_time">&nbsp;</label>			              <br/>			              <label style="display: inline;">修改时间：</label>			              <label style="display: inline;" id="u_modify_time">&nbsp;</label>		            	</p>		            	<p>			              <label style="display: inline;">妮称：</label>			              <input class="text-input small-input" type="text" name="nickName" />			            </p>			            <p>			              <label style="display: inline;">真实姓名：</label>			              <input class="text-input small-input" type="text" name="realName" />			            </p>			            <p>			              <label style="display: inline;">联系电话：</label>			              <input class="text-input small-input" type="text" name="phone" />			            </p>			            <p>			              <label style="display: inline;">Email：</label>			              <input class="text-input small-input" type="text" name="email" />			            </p>			            <p>			              <label style="display: inline;">QQ：</label>			              <input class="text-input small-input" type="text" name="qq" />			            </p>			            <p>			              <label style="display: inline;">国家：</label>			              <input class="text-input small-input" type="text" name="country" />			            </p>			            <p>			              <label style="display: inline;">地址：</label>			              <input class="text-input small-input" type="text" name="address" />			            </p>			            <p>			              <label style="display: inline;">用户类型：</label>			              <select name="type">								<option value="0">普通会员</option>								<option value="1">门市会员</option>								<option value="2">VIP会员</option>								<option value="3">VIP2会员</option>								<option value="4">VIP3会员</option>								<option value="5">代理会员</option><!--kai 20150916 代理会员可代发他人包裹 -->														  </select>			            </p>			            <p style="margin-left: 50px;">			              <input id="bt_modify_user" class="button" type="submit" value="提交" />			              <input id="bt_modify_user_reset" class="button" type="reset" value="重置">			            </p>		            </fieldset>		            <div class="clear"></div>		            <!-- End .clear -->		          </form>		        </div>			</div>		</div>		</div>	</body><script type="text/javascript">	$("#main-content").ready(function() {		loadNav33Data();	});</script></html>