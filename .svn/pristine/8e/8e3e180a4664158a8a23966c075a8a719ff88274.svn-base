<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%><%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%><!DOCTYPE html><html xmlns="http://www.w3.org/1999/xhtml"><head></head><body>	<div class="block">		<p class="block-heading" data-toggle="collapse"			data-target="#chart-container" style="font-size:20px">全部运单</p>		<div class="box_content">			<div class="content_title">全部运单&nbsp;&nbsp;<a href="#" onclick="javascript: forwardPage('order', 1);">未付款<span style="margin: 0 3px; color: #f00;">(<label id="countOrderState1">0</label>)</span></a></div>			<input type="hidden" id="searchUrl">						<div class="content_text">				<div class="tab_box">					<div class="query">						运单号：<input type="text" name="oid" maxlength="16" />						模糊搜索：<input type="text" name="queryLike" id="queryLike" maxlength="16" />						<select id="stateSelect">							<option value="" selected>全部</option>							<option value="-10">网上置单</option>									<option value="0">等待审核</option>									<option value="1">未付款</option>									<option value="2">已入库</option>									<option value="3">送往集散中心</option>									<option value="4">集散中心已入库</option>									<option value="5">送往机场</option>									<option value="6">空运中</option>									<option value="7">海关清关</option>									<option value="8">目的地第三方快递</option>									<option value="9">收件人已接收</option>													</select>						&nbsp;&nbsp;						<a name="a_search_link" class="btn"							style="background: #007FFF;color: #FFFFFF;display:inline">查询</a>					</div>					<table id="datatable" class="table">						<thead>							<tr>								<th style="width:60px">运单号</th>								<th>货物品名</th>								<th>品牌</th>								<th style="width:60px">收件人</th>								<th>创建日期</th>								<th style="width:40px">运费</th>								<th style="width:60px">当前状态</th>								<th style="width:30px">选择</th>							</tr>						</thead>						<tbody>						</tbody>						<tfoot>							<tr>								<td colspan="8" align="right">共<span id="trsize">0</span>条									<a id="first_page" href="1" name="to_page">首页</a> | <a									id="pre_page" name="to_page">上一页</a> | <input type="text"									class="bor" size="2" /> /<span id="tpsize">0</span>| <a									id="next_page" name="to_page">下一页</a> | <a id="last_page"									name="to_page">未页</a>&nbsp;&nbsp; <a name="a_pay_link"									class="btn" href="#"									style="background: #007FFF;color: #FFFFFF;padding: 5px 15px;display:inline">支付</a>|<a									onclick="xiangqing()" class="btn" href="#"									style="background: #007FFF;color: #FFFFFF;padding: 5px 15px;display:inline">包裹详情</a><!-- |<a									onclick="xiangqing()" class="btn" href="#"									style="background: #007FFF;color: #FFFFFF;padding: 5px 15px;display:inline">包裹状态</a> -->								</td>															</tr>						</tfoot>					</table>					<div>					<p ><strong>注意：</strong></p>					<p ><span style="color:blue;"><strong>支付：</strong></span><span style="color:red">&nbsp;只对状态为"未付款"才有效!</span></p>					<p ><span style="color:blue;"><strong>包裹详情:</strong></span><span style="color:red">&nbsp;只能选择一条运单!</span></p>					</div>				</div>			</div>		</div>	</div></body></html>