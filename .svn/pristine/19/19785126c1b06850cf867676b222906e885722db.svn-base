<script type="text/javascript">
	$(document).ready(function(){
		var isLogin = $("#is_login").val();
		if(isLogin=="1"){
			window.location.href = "user/index.jsp";
		}
		$("#cd-signup").keydown(function(event) {
			if (event.which == 13) {
				login();
			}
		});
		$("#code").click(function(){
			resetValidateCode(this);
		});
		resetValidateCode($("#code"));
	});
	function resetValidateCode(event) {
		$(event).attr('src','code/generate?timestamp=' + new Date().getTime());
	}


	function login() {
		var account = $("#login_telphone").val();
		var password = $("#login_password").val();
		var code = $("#login_verifycode").val();
		
		if(!validateAccount(account)||!validatePwd()){
				return false;
		}
		
		$.ajax({
			type:"post",
			url:"user/login",
			data:{
				"account":account,
				"password":password,
				"vcode":code
			},
			success:function(response) {
				var code = response.code;
				
				if (code == "200") {
					window.location.href = "user/index.jsp";
					
				} else {
					alert("登录失败，失败原因是:" + response.message);
					resetValidateCode($("#code"));
				}
			}
		});
		return false;
	
	}
	
	function validateAccount(account){
		if(!checkUserPhone(account)&&!checkUserEmailRegex(account)){
			alert("手机号或者邮箱是无效的,请输入正确的手机或者邮箱.");
			return false;
		}
		return true;
	}
	function validatePwd(password){
		if(!checkUserPassword(password)){
			alert("密码长度不对.");
			return false;
		}
		return true;
	}
	
	//function register_all(){
	//	register();
		//emailReg();
	//	return true;
	//}
	 
	function register_all(){
		if ( document.getElementsByName('inlineRadioOptions')[0].checked ==true ){
		
			emailReg();
		}
		//if ( document.getElementsByName('inlineRadioOptions')[1].checked ==false ){
		else{
			register();
		}

		return true;
	} 
</script>
