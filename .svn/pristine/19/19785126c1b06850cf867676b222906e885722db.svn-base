<script type="text/javascript">
    function LoginOnKeyDown(obj, event) {
        // 输入回车时，焦点转移同一行的重量输入框
        if (event.keyCode == '13') {
            if (event.preventDefault) {
                event.preventDefault();
            }
            else {
                event.returnValue = false;
            }

            try {
                WebForm_DoPostBackWithOptions(new WebForm_PostBackOptions("ctl00$MainContent$LoginButton", "", true, "LoginUserValidationGroup", "", false, true));
                return false;
            }
            catch (e) {
                alert(e);
                return;
            }
        }
    }


