#u5401{
	background: white;
	padding: 0px;
	margin: 0px;
	border-collapse: 0px;
	border-spacing: 0px;
	border: 0px;
}

#u5401 tr {
	background: white;
	border: 0px;
}

#u5401 td,
#u5401 th {
	background: white;
	border: 2px solid #999;
}

#u5401 div th {
	background: #ef9;
}

#u5402 {
	height: 200px;
	margin-bottom: 10px;
	max-height: 200px;
    overflow: auto;
    overflow-x: hidden;
    border: 1px solid #999;
}

#u5402 table {
	table-layout: fixed;
}

#msgtable tbody tr td {
	word-wrap:break-all;
	white-space: pre-wrap;
}
