<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%><html>	<head>	<link rel="shortcut icon" href="../img/favicon.ico" /> 	</head>	<body>		<div class="block">		<p class="block-heading" data-toggle="collapse"			data-target="#chart-container" style="font-size:20px">转运单路由状态信息			<a  href="#" id="a_back_route_list_link" class="button">返回列表</a>			</p>		<div class="box_content">			<div class="content_title">导入转运单路由状态信息</div>			<div class="content_text">							<div class="tab-content default-tab" id="tab1">					<form id="import_weiyistate_form" action="${pageContext.request.contextPath }/admin/order/import"  method="post" enctype="multipart/form-data">			            <fieldset>				           				            				            <p>				            <label style="display: inline;">导入状态</label>				             <select id="import_tran_state_ex" name="import_tran_state_ex">				            <option value="-7">转运入库</option>									<option value="-6">转运出库</option>									<option value="-5">待检入库</option>									<option value="3">等待付款</option>									<option value="5">分拣完成</option>				            				            </select>				            &nbsp;&nbsp;				            				            <label style="display: inline;">导入仓库</label>				             <select id="import_self_wid_ex" name="import_self_wid_ex">				            </select>				             &nbsp;&nbsp;				             				             <label style="display: inline;" id="towid_label">去往仓库</label>				             <select id="import_to_wid_ex" name="import_to_wid_ex">				            </select>				             &nbsp;&nbsp;				            				            				              <label style="display: inline;">转运单EXCEL文件：</label>				              <input type="file" name="file" class="text-input"/>				              <input id="import_tran_state_bt" class="button" type="submit" value="导入订单状态" />				             				            </p>				         </fieldset>				         <div class="clear"></div>			         </form>					<div class="notification information png_bg">	      				<div>	      					      				请确认导入的文件是否是定义的文件格式(必须使用2003的excel表格，后缀名是:xls)，如果不是的话，可能会导入不成功，如果您不知道导入文件的格式，请下载文件模板。	      				请点击<a href="${pageContext.request.contextPath }/admin/tran/download_excelstate">运单状态模板文件</a>下载文件!	      				</div>	    			</div>				</div>			</div>		</div>		</div>		<!-- 不能删除此div -->		<div id="tmpmsgdiv" style="display:none;">		</div>							</body>	<script type="text/javascript">		$("#main-content").ready(function() {			// kai 20150919-1 //装载仓库及相关联的数据			loadconinfo_tran();			$("#import_tran_state_ex").change(function() {				var state=$("#import_tran_state_ex").val();				if((state=="-7")||(state=="-6"))				{										$("#import_to_wid_ex").show();					$("#towid_label").show();				}				else				{					$("#import_to_wid_ex").hide();					$("#towid_label").hide();									}							});						//导入操作接口			$("#import_tran_state_bt").click(function() {				$("#import_weiyistate_form").ajaxSubmit({					type : 'post',					timeout : 600000,					//url : admin_import_order_state_url,					url : admin_import_tran_order_state_url,					success : function(data) {						//alert(data);						//alert(data.code);																		$("#tmpmsgdiv").html('' + data);						var str = $("#tmpmsgdiv pre").html();						var obj = $.parseJSON(str);						var code = obj.code;						if (code == "200") {							alert("操作完成");							// $("#nav55").click();							nav55Click();						} else if (code == "607") {							alert("您尚未登录或登录已失效！");							logout();						} else {							alert("导入运单失败，原因是:" + obj.message);						}						return false;																						},					error : function(XmlHttpRequest, textStatus, errorThrown) {						alert("操作失败");						return false;					}				});				return false;			});		});				function loadconinfo_tran() {			$.ajax({				type : "get",				async : true,				url : "../warehouse/all",				success : function(response) {					var code = response.code;					//alert(code);					if (code == '200') {						if (response.data != null) {							var str = "";							var str1="";							$.each(response.data, function() {								if($("#admin_id_flag").val()!="admin")								{									if($("#store_id_flag").val()==this.id)									{										str += "<option value='" + this.id + "'>" + this.name										+ "</option>";									}									else									{										str1 += "<option value='" + this.id + "'>" + this.name												+ "</option>";									}								}								else								{									str += "<option value='" + this.id + "'>" + this.name									+ "</option>";									str1 += "<option value='" + this.id + "'>" + this.name									+ "</option>";								}							});																																									$("select[id='import_self_wid_ex']").html(str);								$("select[id='import_to_wid_ex']").html(str1);																				}					}				}			});		}	</script></html>