li.photo_item span del { color:#010101; }
li.photo_item span strong { color:#666; }

ul.product_list {  }
ul.product_list li.photo_item { width:165px; height:210px; float:left; text-align:left; padding:15px 15px; }
ul.product_list li.photo_item a.thumb img { width:160px; height:160px; border:solid 1px #ccc; }
ul.product_list li.photo_item a.thumb:hover img { border:solid 1px #666; }

/*在线客服*/
div.simple_online { background:#fff url('../img/online.jpg') center 98% no-repeat; border:solid 1px #ccc; border-radius:3px; -moz-border-radius:3px; -o-border-radius:3px; -webkit-border-radius:3px; width:136px; 
                    padding:4px 4px 68px 4px; font-size:12px; line-height:18px; position:absolute; right:5px; top:15px; z-index:9999;
                    -webkit-box-shadow: 0 0px 7px rgba(0,0,0,0.3);
                    -moz-box-shadow: 0 0px 7px rgba(0,0,0,0.3);
	                box-shadow: 0 0px 7px rgba(0,0,0,0.3); }
    div.simple_online h2 { background: #5d5a5a; color: #fff; height:26px; line-height:26px; font-size: 14px; text-align: center;
                           border-top-left-radius:3px; 
                           border-top-right-radius:3px;
    }
    div.simple_online ul { margin: 10px; }
        div.simple_online ul li { line-height: 30px; vertical-align:middle; }
            div.simple_online ul li img { vertical-align: middle; margin-right:5px; }
    div.simple_online hr { border:none; border-top:dashed 1px #ccc; overflow:hidden; height:1px; }
    div.simple_online p { padding: 5px; color:#f00; }
	
