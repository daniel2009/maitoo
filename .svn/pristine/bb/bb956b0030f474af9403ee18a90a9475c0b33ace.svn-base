/*---------普通菜单--------*/
#menu { background:url('../img/menu.gif') no-repeat; height:37px; line-height:37px; }
#menu a,#menu a:link,#menu a:visited { cursor:pointer; color:#fff; width:100px; height:37px; display:inline-block; float:left; 
                                       background:url('../img/splitter.gif') right top no-repeat; }
#menu a:hover { text-decoration:none; color:#eee; }
#menu a span { display:block; width:80px; height:37px; text-align:center; font-size:14px; margin-left:10px; margin-right:12px; line-height:35px; overflow:hidden; }
#menu a span.end { background:none; }
#menu a span.active { line-height:35px; font-weight:bold; background:url('../img/hover.gif') left 3px no-repeat; color:#06d; }
/*---------------下拉菜单---------------*/
#navMenu { height:36px; width:auto; overflow:hidden; }
#navMenu dl { line-height:36px; font-size:14px; float:left; }
#navMenu dl a:hover { text-decoration:none; }
#navMenu dl dt{ display:none; }
#navMenu dl dd ul li { font-weight:bold; }
#navMenu dl dd ul li a { margin:0px 10px; border-bottom:solid 1px #fff; padding:0; }
#navMenu dl dd ul li a:hover { color:#f12710; background:none; }
#navMenu dl dd ul li a span.active { color:#f60; }

#navMenu dl dd{ width:130px; float:left; text-align:center; background:url('../img/menu_bg.gif') right center no-repeat; }
#navMenu dl dd a { color:#fff; display:block; line-height:36px; }
#navMenu dl dd a:hover,#navMenu dl dd a.active { background:url('../img/menu_bg2.gif') right center no-repeat; }
#navMenu dl dd.menu-home a { background-image:url('../img/home.gif'); }
#navMenu dl dd.menu-home a:hover,#navMenu dl dd.menu-home a.active { background-image:url('../img/home1.gif');  }
#navMenu dl dd.menu-end a { background-image:url('../img/end.gif'); }
#navMenu dl dd.menu-end a:hover,#navMenu dl dd.menu-end a.active { background-image:url('../img/end2.gif');  }

.dropMenu{ position:absolute;top:0; z-index:10000;width:180px;visibility: hidden;background:#520914; filter:alpha(opacity=80);-moz-opacity:0.8; padding-bottom:10px; }
.dropMenu li{ text-align:left; color:#fff;line-height:30px; }
.dropMenu li a,#navMenu dl dd.menu-home li a {color:#fff;display:block;zoom:1; background:none; }
.dropMenu li a:hover,#navMenu dl dd.menu-home li a:hover{ background:none; color:#f12710; }


