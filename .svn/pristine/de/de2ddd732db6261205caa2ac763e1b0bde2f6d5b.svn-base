@charset "utf-8";
/*global set*/
.clearfix:after{clear:both;display:block;content:".";height:0;visibility:hidden;font-size:0;line-height:0;}
.clearfix{display:inline-block;}/* Hides from IE-mac\*/
* html .clearfix{height:1%;}
.clearfix{display:block;}/* End hide from IE-mac*/
div,p,li,dt,dd,label,h1,h2,h3,h4,h5,h6{text-align:left;}
input:focus {outline:none;}

.xlobo_en {font-family:arial;}
/*--- bg ---*/
.query_online i,.serv_project_list i,.solotion_sprial dt s,
.event_list li i,.event_list li s,.service_thumb,.solut_list p i,.event_list1 li s,.event_list1 li i,
.service_detail h2,.service_detail .hd .close,
.serv_opt_list i,.xlobo_notice_list i,.source_download_list i {background:url(../img/sprite.png) no-repeat;}
.solutions_list li i {background:url(../img/icon_fa.png) no-repeat;}
.ly_in {min-width:1000px;max-width:1200px;margin:0 auto;}
/*--- header ---*/
.xlobo_hd {height:71px;background:url(../img/hd_bg.jpg) no-repeat 50% 0;}
.xlobo_hd .ly_in {position:relative;height:71px;background:url(../img/hd_bg.jpg) no-repeat -99px 0;}
.xlobo_hd h1 {float:left;width:195px;height:42px;margin:13px 57px 0 14px;}
.xlobo_hd h1 a {display:block;height:42px;line-height:200em;overflow:hidden;}
.main_nav {float:left;width:655px;margin:42px 0 0 0;}
.main_nav_list li {float:left;width:130px;height:18px;line-height:18px;color:#fff;font-size:14px;text-align:center;border-right:1px solid #857a74;}
.main_nav_list li a {color:#fff;}
.main_nav_list li.last {border-right:0 none;}
.main_nav_list li.cur a,.main_nav_list li a:hover {color:#ffb973;}
.user_opt {position:absolute;right:10px;top:5px;height:20px;text-align:right;}
.user_opt a {display:inline-block;margin-left:8px;color:#ebdac0;}
.user_opt a:hover {color:#ddd;text-decoration:underline;}
/*--- main ---*/
.main {width:100%;background-color:#eee;}
.main .ly_in {*zoom:1;}
.main .ly_in::after {clear:both;display:block;content:".";height:0;visibility:hidden;font-size:0;line-height:0;}
.serv_banner {height:89px;margin-bottom:12px;background:url(../img/line2.png) repeat-x;}
.serv_banner span {display:block;height:54px;padding:35px 0 0 14px;color:#b77719;font-size:28px;background:url(../img/b1.jpg) no-repeat 160px 0;}
/*--- 加盟合作 ---*/
.affiliate {height:394px;padding:0 2px;}
.affiliate_box {padding-left:14px;}
.affiliate p {margin-top:15px;color:#868686;}
.affiliate p a {color:#007fff;}
/*--- 常见问题问答 ---*/
.qa_box {padding-bottom:60px;}
.qa_list {padding-left:14px;}
.qa_list li {padding:10px 0;border-bottom:1px solid #ddd;}
.qa_list p.question {color:#c48d3f;font-size:14px;}
.qa_list p.answer {padding:3px 0 0 28px;color:#858585;}
.qa_list p i {float:left;_display:inline;margin:-3px 0 0 -28px;font-style:normal;font-size:14px;}
.qa_list p a {color:#007fff;}
/*--- 服务协议 ---*/
.serv_agreement {padding-bottom:60px;}
.serv_agreement_box {padding-left:14px;}
.serv_agreement p {text-indent:2em;color:#868686;}
.serv_text {padding:15px 0 0 0;}
.serv_agree_list .sa_hd {height:30px;line-height:30px;}
