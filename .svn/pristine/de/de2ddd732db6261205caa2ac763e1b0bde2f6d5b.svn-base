.serv_agree_list .sa_bd a {color:#007fff;text-decoration:underline;}
.serv_agreement h4 {color:#868686;font-weight:700;}
/*--- worth_banner ---*/
.worth_banner {height:150px;background:#0E2338 url(../img/b2.jpg) no-repeat right 0;overflow:hidden;}
.worth_banner .ly_in {height:130px;padding:20px 0 0 0;}
.worth_banner h2 {padding-left:15px;color:#fff;font-size:28px;}
.worth_banner p {padding-left:15px;color:#999;font-size:14px;}
/*--- 服务中心 ---*/
.query_online {position:relative;width:583px;height:117px;margin:15px auto 0;}
.query_online i {position:absolute;left:0;top:0;z-index:1;display:block;width:129px;height:117px;}
