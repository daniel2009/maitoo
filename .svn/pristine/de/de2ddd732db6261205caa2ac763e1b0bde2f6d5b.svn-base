.serv_opt_list {margin-right:-15px;*zoom:1;overflow:hidden;}
.serv_opt_list::after {clear:both;display:block;content:".";height:0;visibility:hidden;font-size:0;line-height:0;}
.serv_opt_list li {float:left;width:72px;height:100px;margin-right:27px;line-height:1.4;}
.serv_opt_en .serv_opt_list li {height:120px;}
.serv_opt_list a {display:block;height:100px;text-align:center;color:#666;}
.serv_opt_list i {display:block;height:86px;}
.serv_opt_list li.so1 i {background-position:0 -158px;}
.serv_opt_list li.so2 i {background-position:-73px -158px;}
.serv_opt_list li.so3 i {background-position:-146px -158px;}
.serv_opt_list li.so4 i {background-position:-219px -158px;}
.xlobo_notice {padding:0 18px;}
.xlobo_notice h3 {margin-bottom:8px;color:#545454;font-size:14px;}
.xlobo_notice_list li {height:20px;margin-bottom:5px;line-height:20px;}
.xlobo_notice_list a {float:left;width:285px;white-space:nowrap;text-overflow:ellipsis;overflow:hidden;color:#666;}
.xlobo_notice_list i {float:left;width:8px;height:8px;margin:6px 4px 0 0;background-position:-130px -8px;}
.xlobo_notice_list span {float:right;width:70px;text-align:right;color:#bababa;}
/*--- 合作伙伴 ---*/

.friend_box {height:160px;border-top:1px solid #ddd;background-color:#fff;}
.friend_tit {float:left;height:80px;padding-right:20px;line-height:80px;color:#666;}
.friend_list li {float:left;width:150px;margin:10px 10px 0 0;}
.friend_list img {width:148px;height:58px;border:1px solid #eee;}
/*--- 资源下载 ---*/
.source_download {height:375px;}
.source_download .hd {height:32px;border-bottom:1px solid #ddd;}
.source_download h3 {height:32px;line-height:32px;color:#c48c3f;font-size:14px;}
.source_download .bd {padding:15px 0 0 0;}
.source_download_list {padding-left:4px;}
.source_download_list li {height:20px;margin-bottom:5px;line-height:20px;}
.source_download_list a {color:#007fff;text-decoration:underline;}
.source_download_list i {display:inline-block;width:16px;height:16px;margin-right:3px;vertical-align:middle;background-position:-130px -63px;}


        
.ly-ib {display:inline-block}
.ly-block {display:block;}
.ly-hide {display:none;}
.ly-hidden {visibility:hidden;}
.ly-vm {vertical-align:middle}
.ly-fl {float:left;}
.ly-fr {float:right}
.ly-rel {position:relative}
.ly-abs {position:absolute}
