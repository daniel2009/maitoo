<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%><%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%><!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"><html xmlns="http://www.w3.org/1999/xhtml"><head><title>运单查询-快递中美专线</title><link rel="shortcut icon" href="img/favicon.ico" /> <style type="text/css">.main {	width: 900px;	margin: 0 auto;	padding-top: 20px;	overflow: hidden;}.main_box {	margin-top: 10px;}.main_box_right .login {	width: 231px;	height: 250px;	margin: 50px 335px;	border: 1px #ccc solid;}.main_box .box_title {	text-align: center;	line-height: 22px;}.ytable {	font-size: 11px;	color: #042023;	border-collapse: collapse;	border: 1px solid #fff;	margin: -1px;	width: 800px;	margin-left: 50px;}.ytable tr th {	padding: 6px 8px;	background: #aaa;	border: 1px solid #fff;	font-weight: bold;}.ytable tr td {	padding: 5px;	border: 1px solid #fff;	background: #ddd;}.yun {	height: 80px;	width: 460px;	border: #CCCCCC 1px solid;	line-height: 80px;	margin: 20px 250px;	padding: 0 20px;}.yun input {	margin: 0 10px;	height: 30px;	width: 200px;}</style><script type="text/javascript" src="js/jquery.js"></script><script type="text/javascript">	$(function(){ 		// 开始获取数据		//update by chenkanghua  订单号为 WY+8位+US		var value = $(":hidden[name='s_oid']").val();		var arrs=value.split(";"); 		if (arrs!=null) {			search(arrs);		}				$("a[name='a_search_route_link']").click(function(){			var value = $(":text[name='orderId']").val();			var arrs=value.split(";"); 			if (arrs!=null) {				search(arrs);			}						return false;		});	});		function search(arrs) {		$.ajax({			type:"get",			url:"route/get_by_guest",			data : $.param({				"oid" : arrs,			}, true),			success : function(response) {				var code = response.code;				if ("200" == code) {					showRouteList(response.data);				} else {					alert("获取运单路由状态失败，原因是:" + response.message);				}				return false;			}		});	}		function showRouteList(map) {		$("#ydroutetable tbody").html("");		if (map != null) {			for (var key in map) {				var value = map[key];				if (value == null || value.length == 0) {					var str = "<tr><td colspan='4'>" + key + "运单没有对应的路由信息或者是该运单号错误！</td></tr>";					$("#ydroutetable tbody").append(str);				} else {					$.each(value, function(){						var str="<tr>";						str+="<td>" + this.orderId + "</td>";						str+="<td>" + this.date + "</td>";						str+="<td>" + this.state + "</td>";						str+="<td>" + this.remark + "</td>";						str+="</tr>";						$("#ydroutetable tbody").append(str);					});				}			}		}	}</script></head><body><input type="hidden" name="s_oid" value="${param.oid }"/><div class="main">	<div class="main_box">		<div class="yun">			快递查询：<input name="orderId" type="text" maxlength="16" value="${param.oid }"/><a class="btn" name="a_search_route_link" href="#">查询</a>		</div>		<table id="ydroutetable" class="ytable">			<thead >				<tr >					<th style="background-color:#ADD8E6; width:150px;">运单单号</th><th style="background-color:#87CEEB">时间</th><th style="background-color:#87CEFF">状态</th><th style="background-color:#5CACEE">备注信息</th>				</tr>			</thead>			<tbody>			</tbody>		</table>	</div></div><!--end main--><br/><br/><br/><br/></body></html>