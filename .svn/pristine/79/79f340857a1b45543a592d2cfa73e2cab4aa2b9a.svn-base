	font-weight: bold;
	text-align: center;
}
/*alert*/
.alert {
	font-size: 14px;
	padding: 10px;
	margin-bottom: 15px;
}
.alert h4 {
	margin-top: 0;
	color: inherit;
}
.alert .alert-link {
	font-weight: bold;
}
.alert > p,
.alert > ul {
	margin-bottom: 0;
}
.alert > p + p {
	margin-top: 5px;
}
.alert-dismissable,
.alert-dismissible {
	padding-right: 35px;
}
.alert-dismissable .close,
.alert-dismissible .close {
	position: relative;
	top: -2px;
	right: -21px;
	color: inherit;
}
.alert-danger {
	color: #a94442;
	background-color: #f2dede;
}
.alert-danger .alert-link {
	color: #843534;
}
.alert-success{
	color: #468847;
	background-color: #dff0d8;
}
.alert-info {
	color: #3a87ad;
	background-color: #d9edf7;
}
/*wala add*/
.wala{
	text-align: center;
	margin-top: 50px;
}


/*底部信息*/
