/*page*/
.paging-full {
	position: relative;
	height: 30px;
	margin-right: 3px;
}
.paging-full .items {
	display: inline;
	float: left;
	margin-top: 4px;
}
.paging-full .item{
	position: relative;
	display: inline;
	float: left;
	min-width: 30px;
	_width: 30px;
	height: 22px;
	color: #3e3e3e;
	text-align: center;
	line-height: 22px;
	border: 1px solid #fff;
	cursor: pointer;
	-webkit-transition: color .1s linear;
	-moz-transition: color .1s linear;
	-ms-transition: color .1s linear;
	-o-transition: color .1s linear;
	transition: color .1s linear;
}
.paging-full .item a{
	display: inline-block;
	min-width: 30px;
	_width: 30px;
	height: 22px;
	line-height: 22px;
}
.paging-full .item:hover {
	color: #f09151;
	border-color: #f09151;
}
.paging-full .prev {
	width: 28px;
	margin-right: 5px;
	margin-left: 0;
}
.paging-full .active, .paging-full .active:hover {
	position: relative;
	background: #f09151;
	color: #fff;
	border-color: #f09151;
	cursor: default;
	text-decoration: none;
}
.paging-full .prev-disabled, .paging-full .prev-disabled:hover {
	cursor: default;
}
.paging-full .dot {
	border: none;
	line-height: 14px;
	cursor: default;
}
.paging-full .next {
	margin-left: 5px;
}
.paging-full .total {
	float: left;
	line-height: 39px;
	margin-left: 5px;
}
.paging-full .form {
	float: left;
}
.paging-full .input {
	width: 38px;
	border: 1px solid #ededed;
	height: 21px;
	text-align: center;
}
.paging-full .button {
	height: 23px;
	width: 43px;
	background: #f09151;
	border-radius: 2px;
	-webkit-border-radius: 2px;
	-moz-border-radius: 2px;
	color: #fff;
	text-align: center;
	line-height: 23px;
	cursor: pointer;
}
.paging-full .button, .paging-full .input, .paging-full .text, .paging-full .total {
	margin-left: 5px;
}
.paging-full .m-more, .paging-full .text, .paging-full .total {
	float: left;
	_display: inline;
	line-height: 30px;
}
.paging-full .button, .paging-full .input {
	float: left;
	_display: inline;
	margin-top: 2px;
}
.paging-full .icon {
	display: inline-block;
	width: 6px;
	height: 9px;
	overflow: hidden;
	background: url(../images/sprite_user.png) no-repeat;
}
.paging-full .prev .icon {
	background-position: -20px -233px;
}
.paging-full .prev:hover .icon {
	background-position: -20px -253px;
}
.paging-full .next .icon {
	background-position: 0 -233px;
}
.paging-full .next:hover .icon {
	background-position: 0 -253px;
}
/*pop*/
.ui-mask {
	position: fixed;
	top: 0px;
	left: 0px;
	width: 100%;
	height: 100%;
	z-index: 999;
	opacity: 0.7;
	filter:progid:DXImageTransform.Microsoft.gradient(enabled='true',startColorstr='#B2000000', endColorstr='#B2000000');
}
:root div.ui-mask {
	filter:none;
	background-color:rgba(0,0,0,0.7);
}
.pop-box-wrap{
	position: fixed;
	left : 50%;
	z-index: 99;
}
.pop-xbox-show {
	width: 700px;
	z-index: 999;
	position: fixed;
	margin-left: -350px;
	box-shadow: 0 4px 16px rgba(0,0,0,.2);
	background: #fff;
	background-clip: padding-box;
	border: 1px solid #cecece;
	outline: 0;
	-webkit-transform: none;
	-moz-transform: none;
	-ms-transform: none;
	transform: none;
	opacity: 1;
}
:root .pop-box-wrap .pop-xbox {
	FILTER: none\9;
}
.pop-close {
	color: #999;
	cursor: pointer;
	display: block;
	font-size: 24px;
	height: 18px;
	line-height: 14px;
	position: absolute;
	right: 16px;			
	top: 16px;
	z-index: 10;
}
.pop-close:hover {
	text-decoration: none;
}
.pop-loading {
	border-radius: 10px;
	position: absolute;
	top: -2px;
	top: 0\0;
	left: 0;
	background: #CBFE1C;
	box-shadow: 0 0 12px #CBFE1C;
	height: 2px;
	zoom: 1;
	overflow: hidden;
}
.pop-content {
	zoom: 1;
	background: #fff;
}
.pop-content .sublimt{
	text-align: center;
}
.pop-content .hd {
	margin-bottom: 7px;
}
.pop-content .hd .tit{
	float: left;
	font-size: 26px;
	font-weight: normal;
	color: #27a1d5;
}
.pop-content .con{
	max-height: 480px;
	overflow-y: scroll;
}
.pop-content .btn {
	display: inline-block;
	min-width: 90px;
	_width: 90px;
	padding: 0 10px;
	margin-right: 10px;
	height: 30px;
	line-height: 30px;
	color: #fff;
	text-align: center;
	border: 1px solid #f09151;
	background-color: #f09151;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px;
}
.pop-content .btn:hover {
	text-decoration: none;
}
.pop-content .btn.btn-light{
	color: #333;
	border-color: #ccc;
	background-color: #fff;
}
/*pop history state*/
.pop-historyState{
	width: 700px;
	padding: 50px;
	margin-left: -400px;
}
.tit-historyState{
	padding-left: 35px;
	background: url(../images/sprite_user.png) no-repeat 0 -449px;
}
.pop-historyState .con{
	padding: 30px;
	border-top: 1px solid #d9d9d9;
}
.pop-historyState li{
	position: relative;
	width: 530px;
	height: 110px;
	padding-left: 110px;
	overflow: hidden;
	background: url(../images/history_state.png) no-repeat;
}
.pop-historyState li h3{	
	font-weight: normal;
	font-size: 18px;
	line-height: 2em;
}
.pop-historyState li .info{
	height: 60px;
	padding: 12px 18px;
	margin-top: 15px;
	color: #fff;
	background-color: #ccc;
	-moz-border-radius: 10px;
	-webkit-border-radius: 10px;
	border-radius: 10px;
}
.historyState li.end-wrap{
	background-position: 0 0;
}
.pop-historyState li.middle-wrap{
	background-position: 0 -112px;
}
.pop-historyState li.start-wrap{
	background-position: 0 -224px;
}
.pop-historyState .end-wrap .info{
	background-color: #85c226;
}
.pop-historyState .middle-wrap .info{
	background-color: #76b9e4;
}
.pop-historyState .start-wrap .info{
	background-color: #ed9354;
}
.tooltip-arrow{
	display: inline-block;
	position: absolute;
	top: 50%;
	left: 98px;
	margin-top: -8px;
	border-width: 9px 12px 9px 0;
	border-right-color: #000;
	width: 0;
	height: 0;
	border-color: transparent;
	border-style: solid;
}
.pop-historyState .start-wrap .tooltip-arrow{
	border-right-color: #ed9354;
}
.pop-historyState .middle-wrap .tooltip-arrow{
	border-right-color: #76b9e4;
}
.pop-historyState .end-wrap .tooltip-arrow{
	border-right-color: #85c226;
}
/*pop-confirm*/
.pop-confirm{
