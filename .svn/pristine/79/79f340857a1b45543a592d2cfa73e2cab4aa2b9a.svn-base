/*404*/
.con_404 .wrapper{
	min-height: 600px;
	height: auto;
	_height: 600px;
	background: #fff url(../images/box.png) no-repeat center 350px;
}
.con_404 h1{
	font-size: 100px;
	text-align: center;
	padding: 80px 0 0;
}
.con_404 .des{
	width: 500px;
	margin-left: auto;
	margin-right: auto;
