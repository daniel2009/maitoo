<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%><%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%><!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"><html xmlns="http://www.w3.org/1999/xhtml"><head><link rel="shortcut icon" href="img/favicon.ico" /></head><body><div style="display: none;" id="tmpmsgdiv"></div>		<div class="block" id="update">		<p class="block-heading" data-toggle="collapse"			data-target="#chart-container" style="font-size:20px">修改-用户密码</p>		<div class="box_content">			<div class="content_title">修改-用户密码</div>			<div class="content_text">				<form action="" id="update_form" method="post"					enctype="multipart/form-data">										<table class="itable">						<tr>							<td class="align_right">旧密码：</td>							<td class="align_left"><input name="old_code" type="password"								maxlength="28" /><span class="red">*</span></td>						</tr>						<tr>							<td class="align_right">新密码：</td>							<td class="align_left"><input name="new_code_1" type="password"								maxlength="28" /><span class="red">*</span></td>						</tr>						<tr>							<td class="align_right">再次输入新密码：</td>							<td class="align_left"><input name="new_code_2" type="password"								maxlength="28" /><span class="red">*</span></td>						</tr>											</table>					<div style="text-align:center;">					<input id="add_zengjiadizi_bt" class="button" type="button" style="background: #007FFF;color: #FFFFFF;padding: 5px 15px;display:inline"									value="修改" onClick="resetpwdbyuser()"/> 					</div>										</form>			</div>		</div>	</div></body></html>