@charset "utf-8";
/*--- 内容层 ---*/
.ly_in {min-width:1000px;max-width:1200px;_width:1200px;margin:0 auto;}
/*--- ly_main ---*/
.ly_main {width:100%;padding:10px 0;background:#eee url(../img/main_bg.png) repeat-x;}
.ly_main .ly_in {padding-bottom:60px;*zoom:1;}
.ly_main .ly_in::after {clear:both;display:block;content:".";height:0;visibility:hidden;font-size:0;line-height:0;}
.ly_main_content .hd {height:64px;text-align:left;}
.ly_main_content h2 {height:64px;line-height:64px;}
.ly_main_content .bd {padding:15px 0 30px 0;*zoom:1;background-color:#fff;}
.ly_nobg_content .bd {background:none;}
.ly_main_content .bd:after {clear:both;display:block;content:".";height:0;visibility:hidden;font-size:0;line-height:0;}
.ly_content_body {float:left;width:900px;}
.ly_content_sidebar {float:right;width:290px;}
/*======== bg ========*/
.c_btn_sure_big i,.c_query_date s {background:url(../img/bs_sprite.png) no-repeat;}
/*======== h ========*/
h2 {color:#666;font-size:18px;}
h3 {color:#c68019;font-size:14px;}
/*======== 三角形 ========*/
.c_triangle_down {width:0;height:0;border:5px solid transparent;_border-style:solid dashed dashed dashed;border-top-color:#7a6b61;overflow:hidden;}
/*======== form ========*/
/*--- input ---*/
.c_form_text {height:28px;padding:0 5px;line-height:28px;border:1px solid #ddd;box-shadow:1px 1px 3px rgba(0,0,0,.1) inset;-moz-box-shadow:1px 1px 3px rgba(0,0,0,.1) inset;-webkit-box-shadow:1px 1px 3px rgba(0,0,0,.1) inset;color: #999;}
/*--- select ---*/
.c_form_select {width:128px;height:30px;color:#858585;border:1px solid #ddd;}
@-moz-document url-prefix() { .c_form_select {padding:3px 0 0 0;} }
/*--- textarea  ---*/
textarea {padding:5px;resize:none;border:1px solid #ddd;box-shadow:1px 1px 3px rgba(0,0,0,.1) inset;-moz-box-shadow:1px 1px 3px rgba(0,0,0,.1) inset;-webkit-box-shadow:1px 1px 3px rgba(0,0,0,.1) inset;}
/*--- button ---*/
.c_btn_sure,.c_btn_normal {display:inline-block;height:28px;padding:0 25px;line-height:28px;border-width:1px;border-style:solid;background:url(../img/line3.png) repeat-x;}
.c_btn_sure {color:#C49044;border-color:#d2b284;}
.c_btn_normal {color:#858585;border-color:#ccc;}
.c_btn_sure_big {position:relative;
                 display:inline-block;
                 *display:inline;
                 height:50px;padding:0 65px;
                 color:#fff;font-size:14px;line-height:50px;background-color:#ff8d07;border:none;cursor: pointer;
                 zoom:1;
}
.c_btn_sure_big[disabled]{
    background: #adadad;
    cursor:default;
}
.c_btn_sure_big i {position:absolute;left:0;bottom:-14px;display:block;width:100%;height:14px;overflow:hidden;}
/*--- 公告 ---*/
.c_notice {position:relative;height:36px;line-height:36px;background-color:#fff;overflow:hidden;}
.c_notice_tit {float:left;_width:26px;height:36px;padding:0 12px;color:#fff;text-align:center;background-color:#d2b284;}
.c_notice_list {position:absolute;left:67px;width:90%;}
.c_notice_en .c_notice_list {left:125px;}
.c_notice_list li {float:left;_display:inline;width:30%;height:36px;margin-left:15px;}
.c_notice_list li a {display:block;height:36px;height:36px;color:#868686;white-space:nowrap;text-overflow:ellipsis;overflow:hidden;}
.c_notice_list li a:hover {text-decoration:underline;}
.c_notice_more {position:absolute;right:0;top:0;display:block;width:40px;height:36px;color:#007fff;}
.c_notice_more:hover {text-decoration:underline;}
/*--- tab ---*/
.ly_main_content .c_item_tab {padding:0;background:none;}
.c_item_hd {height:37px;border-bottom:6px solid #d2b284;}
.c_item_tab_list li {float:left;height:37px;margin-right:1px;padding:0 34px;line-height:37px;color:#858585;font-size:14px;font-weight:700;cursor:pointer;text-align:center;background-color:#fff;}
.c_item_tab_list li.cur {color:#fff;background-color:#d2b284;}
.c_item_tab_list li i {display:inline-block;font-size:12px;font-style:normal;}
.c_item_tab .c_item_bd {padding:15px 0 30px 0;background-color:#fff;}
/*--- page_turn ---*/
.c_page_turn {height:30px;}
.c_page_turn a {display:inline-block;width:28px;height:28px;line-height:28px;color:#999;vertical-align:top;text-align:center;border:1px solid #ddd;background-color:#fff;font-family:"\5b8b\4f53";}
.c_page_turn a.cur,.c_page_turn a:hover {color:#fff;border:1px solid #d2b284;background-color:#d2b284;}
.c_page_turn a.c_ellipsis {border:0 none;}
.c_page_turn a.c_ellipsis:hover {color:#999;border:0 none;background:none;}
.c_page_turn a.c_turn_prev,.c_page_turn a.c_turn_next {overflow:hidden;}
.c_page_turn a.c_turn_next {margin-right:5px;}
.c_page_turn input.c_form_text {width:32px;}
.c_page_turn label {margin:0 5px;color:#999;}
.c_page_turn a.c_btn_normal {padding:0 15px;border-color:#ccc;}
.c_page_turn a.c_btn_normal:hover {color:#999;border-color:#ccc;}
.c_page_turn a.c_unavailable,.c_page_turn a.c_unavailable:hover {color:#999;cursor:default;background-color:#eee;border-color:#ccc;}
/*--- 查询表单 ---*/
.c_query_list {margin-bottom:20px;*zoom:1;overflow:hidden;}
.c_query_list::after {clear:both;display:block;content:".";height:0;visibility:hidden;font-size:0;line-height:0;}
.c_query_list li {float:left;margin-right:10px;color:#858585;}
.c_query_list h4 {height:32px;line-height:32px;}
.c_query_list .c_form_text {width:168px;}
.c_query_daterange label {float:left;height:30px;margin:0 5px;line-height:30px;}
.c_query_date {position:relative;float:left;width:180px;height:30px;}
.c_query_date s {position:absolute;right:5px;top:8px;display:block;width:16px;height:16px;cursor:pointer;background-position:-148px -15px;}
.c_query_date .c_form_text {width:148px;padding-right:25px;}
.c_query_form .c_query_btn {*zoom:1;overflow:hidden;}
.c_query_form .c_query_btn a {float:left;margin-right:5px;}
/*--- 数据表格 ---*/
.c_data_wrap {*zoom:1;color:#666;}
.c_data_wrap::after {clear:both;display:block;content:".";height:0;visibility:hidden;font-size:0;line-height:0;}
.c_data_topic {margin:0 10px;height:35px;padding-bottom:7px;border-bottom:1px solid #eee;*zoom:1;overflow:hidden;}
.c_data_topic label {float:left;height:30px;line-height:30px;}
.c_data_sort {float:left;width:235px;}
.c_data_sort select {width:158px;}
.c_data_shownum {float:right;height:20px;margin:10px 0 0 0;line-height:20px;text-align:right;}
.c_data_shownum p {margin-right:-5px;*zoom:1;overflow:hidden;}
.c_data_shownum a {display:inline-block;margin:0 5px;color:#999;}
.c_data_shownum a.cur {color:#c57f19;}
.c_data_table_wrap {margin:0 10px;padding-bottom:7px;}
.c_data_table {width:100%;border-bottom:2px solid #eee;}
.c_data_table th {height:36px;text-align:left;border-bottom:2px solid #eee;}
.c_data_table td {height:45px;border-bottom:1px dotted #ddd;}
.c_data_table td.c_data_tl,.c_data_table th.c_data_tl {text-align:left;}
.c_data_table a {color:#007fff;}
.c_data_table a:hover {text-decoration:underline;}
.c_data_wrap .c_page_turn {float:right;}
.c_page_turn_wrap {padding:0 10px;*zoom:1;overflow:hidden;}
.c_page_total {float:left;height:30px;line-height:30px;}
.c_data_prompt {clear:both;padding:25px 10px 0;}
.c_data_prompt a {display:inline-block;margin-left:5px;color:#007fff;}
.c_data_prompt a:hover {text-decoration:underline;}
.c_data_check_wrap .c_data_topic {border-bottom:0 none;}
.c_batch_operation {height:40px;margin:0 10px;line-height:40px;border:1px solid #eee;}
.c_batch_select {float:left;background:url(../img/line4.png) repeat-x;}
.c_batch_select span {float:left;_width:140px;height:40px;padding:0 12px;text-align:center;border-right:1px solid #eee;}
.c_batch_select a {display:block;height:40px;color:#999;}
.c_batch_page {float:right;height:40px;padding-right:10px;text-align:right;}
.c_batch_page a {display:inline-block;width:18px;height:18px;line-height:18px;color:#878787;text-align:center;border:1px solid #eee;font-weight:700;font-family:"\5b8b\4f53";}
.c_batch_page a.c_batch_unavailable {color:#ddd;cursor:default;}
.c_batch_page b {display:inline-block;height:40px;}
.c_data_check_wrap .c_data_table th.ly_odd_number {padding-left:40px;}
.c_data_check_wrap .c_data_table td.ly_odd_number {padding-left:12px;}
.c_data_check_wrap .c_data_table td.ly_odd_number input {margin-right:14px;vertical-align:-2px;}
.c_data_table .c_data_check {text-align:center;}
.c_remark {display:block;width:120px;white-space:nowrap;text-overflow:ellipsis;overflow:hidden;}
.c_data_table th .c_form_select {width:102px;}

