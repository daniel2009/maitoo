<script type="text/javascript">
    function LoginOnKeyDown(obj, event) {
        // 输入回车时，焦点转移同一行的重量输入框
        if (event.keyCode == '13') {
            if (event.preventDefault) {
                event.preventDefault();
            }
            else {
                event.returnValue = false;
            }

            try {
                WebForm_DoPostBackWithOptions(new WebForm_PostBackOptions("ctl00$MainContent$LoginButton", "", true, "LoginUserValidationGroup", "", false, true));
                return false;
            }
            catch (e) {
                alert(e);
                return;
            }
        }
    }


    function OpenWinMailValideX(un, em, country, cType) {
        $("#iframeForMailValidatex").attr("src", "/Account/Certification/TransferInfo.aspx?un=" + un + "&em=" + em + "&icountry=" + country + "&cType=" + cType);
        $("#MainContent_mailValidatex").dialog({
            autoOpen: true,
            width: 580,
            modal: true,
            close: function () {
            }
        });
    }

     
    $(function () {
        $.cookie('login', 'true', { path: '/' });

        $('.logindiv').hide();
    });
</script>

