<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.meitao.dao.MenshidadanControlDao">	<resultMap id="basic_result_map" type="com.meitao.model.MenshidadanControl">		<id column="id" property="id" javaType="String" />		<result column="storeId" property="storeId" javaType="String"/>		<result column="employeeId" property="employeeId" javaType="String"/>		<result column="modify_date" property="modifyDate" javaType="String"/>		<result column="create_date" property="createDate" javaType="String"/>		<result column="print_items" property="printItems" javaType="String"/>		<result column="keydown_item" property="keydownItem" javaType="String"/>						</resultMap>		<insert id="insert" useGeneratedKeys="true" keyProperty="id">		INSERT INTO menshidadan_control(storeId, employeeId, modify_date,create_date,print_items,keydown_item) VALUES(#{storeId}, #{employeeId}, #{modifyDate}, #{createDate}, #{printItems}, #{keydownItem})	</insert>	<select id="findself" resultMap="basic_result_map">		SELECT * FROM menshidadan_control WHERE storeId=#{storeId} and employeeId=#{employeeId} limit 1	</select>	<update id="update">		update menshidadan_control		set modify_date=#{modifyDate},print_items=#{printItems},keydown_item=#{keydownItem}		where storeId=#{storeId} and employeeId=#{employeeId}	</update></mapper>