<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.meitao.dao.TranshipmentRouteDao">	<resultMap id="basic_result_map" type="com.meitao.model.TranshipmentRoute">		<id column="id" property="id" javaType="String" />		<result column="transhipment_id" property="transhipmentId" javaType="String"/>		<result column="state" property="state" javaType="String"/>		<result column="date" property="date" javaType="String"/>		<result column="warehouse_id" property="warehouseId" javaType="String"/>		<result column="remark" property="remark" javaType="String"/>		<result column="warehouse_name" property="warehouseName" javaType="String"/>	</resultMap>		<insert id="insert" keyProperty="id" useGeneratedKeys="true">		INSERT INTO transhipment_route(transhipment_id, state, date, warehouse_id, remark) VALUES(#{transhipmentId}, #{state}, #{date}, #{warehouseId}, #{remark})	</insert>	<select id="findByTranshipmentBillId" resultMap="basic_result_map">		SELECT r.*,w.`name` AS warehouse_name FROM transhipment_route r LEFT JOIN warehouse w ON r.warehouse_id=w.id WHERE transhipment_id = #{transhipmentId} ORDER BY date DESC	</select></mapper>