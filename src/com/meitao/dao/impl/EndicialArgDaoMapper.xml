<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.meitao.dao.EndicialArgDao">	<resultMap id="basic_result_map" type="com.meitao.model.Endicial_arg">		<id column="id" property="id" javaType="String" />		<result column="accountId" property="accountId" javaType="String"/>		<result column="passPhrase" property="passPhrase" javaType="String"/>		<result column="requesterId" property="requesterId" javaType="String"/>		<result column="labelSize" property="labelSize" javaType="String"/>		<result column="imageFormat" property="imageFormat" javaType="String"/>		<result column="test" property="test" javaType="String"/>		<result column="apiUrl" property="apiUrl" javaType="String"/>		<result column="create_date" property="createDate" javaType="String" />		<result column="modify_date" property="modifyDate" javaType="String" />		<result column="auth_warehouseId" property="authWarehouseId" javaType="String" />		<result column="userprice" property="userprice" javaType="String" />					</resultMap>		<insert id="insert" useGeneratedKeys="true" keyProperty="id">		INSERT INTO endicia_arg(accountId, passPhrase, requesterId, labelSize, imageFormat, test, apiUrl,create_date,modify_date,userprice)		VALUES(#{accountId}, #{passPhrase}, #{requesterId}, #{labelSize}, #{imageFormat},		 #{test}, #{apiUrl}, #{createDate}, #{modifyDate}, #{userprice})	</insert>	<update id="modifybyid" parameterType="com.meitao.model.Endicial_arg">		update `endicia_arg`		set `accountId`=#{accountId},		`passPhrase`=#{passPhrase},		`requesterId`=#{requesterId},		`labelSize`=#{labelSize},		`imageFormat`=#{imageFormat},		`test`=#{test},		`apiUrl`=#{apiUrl},		`modify_date`=#{modifyDate},		`auth_warehouseId`=#{authWarehouseId},		`userprice`=#{userprice}		where id = #{id} 	</update>	<select id="getone" parameterType="java.lang.String"		resultMap="basic_result_map">		select * from endicia_arg limit 1	</select></mapper>